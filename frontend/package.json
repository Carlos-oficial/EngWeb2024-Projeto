{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "all": "npm-run-all --parallel api dev",
    "dev": "next dev",
    "build": "prettier --check . && next build",
    "format": "prettier --write .\ncd ..\nsource backend/.venv/bin/activate\nisort backend && black backend",
    "start": "next start",
    "lint": "prettier --check . && next lint\ncd ..\nsource backend/.venv/bin/activate\npylint backend --errors-only",
    "api": "cd ../backend\ndocker compose up\ncd ..\nexport PYTHONPATH=.\nsource backend/.venv/bin/activate\npython backend/app.py",
    "setup": "cd ..\nexport PYTHONPATH=.\npython -m venv backend/.venv\nsource backend/.venv/bin/activate\npip install -r backend/requirements.txt"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.4.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "next": "14.2.1",
    "next-auth": "^4.24.7",
    "npm-run-all": "^4.1.5",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.5",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.1",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
